 relative;    \r\n  }\r\n\r\n.menu {\r\n\tposition: relative;\r\n\tdisplay: flex;\r\n\tz-index: 2;\r\n}\r\n\r\n.hr {\r\n\tposition: fixed;\r\n\tleft: 50%;\r\n\twidth: 3px;\r\n\theight: 100vh;\r\n\tbackground-color: #000;\r\n\tz-index: 3;\r\n\ttransform: scaleY(0);\r\n}\r\n\r\n.menu-left,\r\n.menu-right {\r\n\tposition: fixed;\r\n\twidth: 50%;\r\n\theight: 100vh;\r\n\tbackground: #a71c1c;\r\n\tcolor: #fff;\r\n\tdisplay: flex;\r\n\tjustify-content: center;\r\n\talign-items: center;\r\n\tz-index: 10;\r\n}\r\n\r\n.menu-left {\r\n\tleft: -50%;\r\n\tz-index: 10;\r\n}\r\n\r\n.menu-right {\r\n\tright: -50%;\r\n\tz-index: 10;\r\n}\r\n\r\n.menu-link a {\r\n\ttext-decoration: none;\r\n\tcolor: #fff;\r\n\tfont-size: 100px;\r\n}\r\n\r\n.menu-link:first-child a::before {\r\n\tdisplay: block;\r\n\tposition: relative;\r\n\tcontent: \"\";\r\n\tbackground-image: url(\"/images/brush-stroke.png\");\r\n\tbackground-size: 100%;\r\n\tbackground-repeat: no-repeat;\r\n\ttop: 80px;\r\n\tleft: 0;\r\n\theight: 40px;\r\n\twidth: 240px;\r\n}\r\n\r\n.menu-link a span {\r\n\tfont-family: \"Poppins\";\r\n\tfont-size: 24px;\r\n}\r\n\r\n.contact .title {\r\n\tfont-size: 80px;\r\n}\r\n\r\n.contact .info {\r\n\tfont-family: Helvetica, sans-serif;\r\n\tfont-weight: lighter;\r\n}\r\n\r\n.mail {\r\n\tmargin-bottom: 60px;\r\n}\r\n\r\n.menu-close {\r\n\tz-index: 2;\r\n\tposition: fixed;\r\n\ttop: 50%;\r\n\tleft: 50%;\r\n\ttransform: translate(-50%, -50%);\r\n\tcursor: pointer;\r\n\topacity: 1;\r\n\tz-index: 11;\r\n}\r\n\r\n.menu-close-img {\r\n\twidth: 140px;\r\n\theight: 140px;\r\n\tborder-radius: 100%;\r\n\tborder: 10px solid #000;\r\n}\r\n\r\nimg {\r\n\twidth: 100%;\r\n}\r\n\r\n/*\ttop: 60px;\r\nleft: 60px;\r\nright: 60px; */\r\n\r\n.menu-open {\r\n\tposition: fixed;\r\n\ttop: 45px;\r\n\tright: 60px;\r\n\tpadding: 1rem;\r\n\tcursor: pointer;\r\n\tfont-size: 20px;\r\n\tfont-weight: bolder;\r\n\tcolor: #818181;\r\n\r\n\tborder: 2px solid;\r\n\tborder-radius: 5px;\r\n\r\n\tz-index: 11;\r\n}\r\n\r\n.seperator {\r\n\theight: 2px;\r\n\tmargin-bottom: 30rem;\r\n}\r\n\r\n.frameworks-seperator {\r\n\theight: 2px;\r\n\tmargin-bottom: .5rem;\r\n}\r\n\r\n.about {\r\n\twidth: auto;\r\n\theight: 100vh;\r\n\tmargin-top: 0px;\r\n\tmargin-left: 0;\r\n\tmargin-right: auto;\r\n\tdisplay: flex;\r\n}\r\n\r\n.about-text {\r\n\tcolor: #fafafa;\r\n\tfont-weight: 200;\r\n\tborder-radius: 10px;\r\n\tline-height: 10rem;\r\n}\r\n\r\n.about-text h2 {\r\n\tfont-weight: lighter;\r\n\tfont-size: calc(5vw + 50px);\r\n\tmargin-top: 5rem;\r\n\tmargin-right: 30rem;\r\n\ttext-decoration: none;\r\n\tcolor: transparent;\r\n\ttransition: color 0.3s, -webkit-text-stroke 0.3s;\r\n\t-webkit-text-stroke: 1px #fafafa;\r\n}\r\n\r\n.about-text p {\r\n\tfont-weight: lighter;\r\n\tfont-size: calc(1vw + 10px);\r\n}\r\n\r\n/* BlotterJS Styling */\r\n\r\n#main {\r\n\tpadding-top: 20vh;\r\n\ttext-align: center;\r\n\tmargin-bottom: -40rem;\r\n}\r\n\r\n/* overlay loader */\r\n\r\n.overlay {\r\n\tz-index: 100;\r\n\tposition: fixed;\r\n\twidth: 100%;\r\n\theight: 100%;\r\n\tbackground: #a71c1cf1;\r\n\ttop: 0%;\r\n\tcolor: #fafafa;\r\n  }\r\n  \r\n  .overlay h1 {\r\n\tposition: absolute;\r\n\ttop: 50%;\r\n\tleft: 50%;\r\n\ttransform: translate(-50%, -50%);\r\n\tfont-size: 150px;\r\n\tfont-weight: bolder;\r\n\tletter-spacing: 20px;\r\n  }\r\n\r\n/* Media Queries */\r\n\r\n@media (max-width: 900px) {\r\n\t.menu-right {\r\n\t\tdisplay: none;\r\n\t}\r\n\r\n\t.menu-left {\r\n\t\twidth: 100%;\r\n\t}\r\n\r\n\t.menu-close {\r\n\t\tposition: fixed;\r\n\t\ttop: 10%;\r\n\t\tleft: 80%;\r\n\t\ttransform: translate(-50%, -50%);\r\n\t\tmargin: 40px;\r\n\t}\r\n\r\n\t.hr {\r\n\t\tdisplay: none;\r\n\t}\r\n\r\n\timg {\r\n\t\twidth: 40%;\r\n\t}\r\n}"},"lineCount":null}},"error":null,"hash":"294801c3b657547fccda19c347912dad","cacheData":{"env":{}}}